{
  "name": "messenger-portal",
  "productName": "Messenger Portal",
  "version": "0.0.1",
  "main": "dist-electron/main/index.js",
  "description": "Messanger app",
  "author": "NR",
  "license": "MIT",
  "private": true,
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
    }
  },
  "win": {
    "target": "nsis",
    "publisherName": "NR"
  },
  "files": ["notification.html"],
  "scripts": {
    "dev": "vite",
    "start": "",
    "build-vite": "tsc && vite build",
    "electron-builder": "electron-builder --publish always",
    "copy": "shx cp notification.html dist",
    "build": "npm run build-vite ",
    "preview": "vite preview",
    "pree2e": "vite build --mode=test",
    "e2e": "playwright test",
    "tester": "nodemon nodeTester.js",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win ",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux",
    "build:no-publish": "npm run build && electron-builder --win --publish never",
    "build:full": "npm run build && electron-builder --win --publish always"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@google-cloud/storage": "^7.9.0",
    "@radix-ui/colors": "^3.0.0",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/themes": "^3.1.6",
    "@types/i18next": "^13.0.0",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "dexie": "^3.2.6",
    "dotenv": "^16.4.7",
    "electron-dl": "^3.5.2",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^5.3.0",
    "express": "^4.18.2",
    "firebase": "10.7.2",
    "framer-motion": "^12.23.12",
    "google-spreadsheet": "^4.1.4",
    "http": "^0.0.1-security",
    "i18next": "^24.2.2",
    "i18next-browser-languagedetector": "^8.0.2",
    "imagekit": "^5.0.0",
    "jotai": "^2.11.3",
    "node-cache": "^5.1.2",
    "node-stream-zip": "^1.15.0",
    "radix-ui": "^1.4.3",
    "react-i18next": "^15.4.0",
    "react-router-dom": "^6.15.0",
    "react-toastify": "^11.0.3",
    "sass": "^1.83.4",
    "sharp": "^0.33.2",
    "shelljs": "^0.7.5",
    "socket.io": "4.8.1",
    "socket.io-client": "^4.7.1",
    "tail": "^2.2.6",
    "tesseract.js": "^5.0.5",
    "uuid": "^9.0.0",
    "vite-plugin-svgr": "^3.2.0",
    "vite-tsconfig-paths": "^4.2.0",
    "winreg": "^1.2.4",
    "zustand": "^4.3.7",
    "@shared/lib": "*"
  },
  "devDependencies": {
    "@playwright/test": "^1.32.0",
    "@types/express": "^4.17.17",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.1.8",
    "@types/shelljs": "^0.8.12",
    "@types/steam": "^0.0.29",
    "@types/steam-client": "^2.5.3",
    "@types/tail": "^2.2.3",
    "@types/uuid": "^9.0.1",
    "@types/winreg": "^1.2.32",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.16",
    "binary-parser": "^2.2.1",
    "electron": "^23.2.0",
    "electron-builder": "^24.9.1",
    "electron-winstaller": "^5.3.0",
    "nodemon": "^3.1.0",
    "postcss": "^8.4.33",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "shx": "^0.3.4",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.0.2",
    "vite": "^4.1.4",
    "vite-plugin-electron": "^0.11.2",
    "vite-plugin-electron-renderer": "^0.14.1"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  }
